{
    "eslint.options": {
      "configFile": ".eslintrc.js"
    },
    "eslint.validate": [
      "javascript",
      "javascriptreact"
    ],
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "editor.formatOnSave": true,
    "javascript.format.enable": false,
    "editor.tabSize": 2
  }